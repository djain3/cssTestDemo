          "_customHTML": "<section class=\"contacts map\" group=\"Contacts\" plugins=\"amp-main, amp-iframe, amp-form, amp-mustache\">\n\t<mbr-parameters>\n\t<!-- Block parameters controls (Blue \"Gear\" panel) -->\n\t<header>Size</header>\n\t<select title=\"Type Block\" name=\"blockType\">\n\t\t<option value=\"1\" selected=\"\">Maps With Contacts</option>\n\t\t<option value=\"2\">Maps With Forms</option>\n\t\t<option value=\"3\">Forms Width Contacts</option>\n\t\t<option value=\"5\">Map</option>\n\t\t<option value=\"6\">Form</option>\n\t\t<option value=\"7\">Contacts</option>\n\t</select>\n\t<input type=\"checkbox\" title=\"Full Width\" name=\"fullWidth\">\n\t<input type=\"range\" inline=\"\" title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"8\" step=\"1\" value=\"2\">\n\t<input type=\"range\" inline=\"\" title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"8\" step=\"1\" value=\"2\">\n\t<input type=\"range\" inline=\"\" title=\"Left/Right Width\" name=\"blockWidth\" min=\"1\" max=\"3\" step=\"1\" value=\"2\" condition=\"(blockType!=7) &amp;&amp; (blockType!=6) &amp;&amp; (blockType!=5)\">\n\t<!--  -->\n\t<header>Show/Hide</header>\n\t<input type=\"checkbox\" title=\"Title\" name=\"showTitle\" checked=\"\">\n\t<input type=\"checkbox\" title=\"Subtitle\" name=\"showSubtitle\" checked=\"\">\n\t<input type=\"checkbox\" title=\"Contacts Title\" name=\"showCardTitle\" checked=\"\">\n\t<input type=\"checkbox\" title=\"Contacts Text\" name=\"showCardText\" checked=\"\">\n\t<!--  -->\n\t<header>Align</header>\n\t<select title=\"Title\" name=\"titleAlign\" condition=\"showTitle || showSubtitle\">\n        <option value=\"1\">Left</option>\n        <option value=\"2\" selected=\"\">Center</option>\n        <option value=\"3\">Right</option>\n    </select>\n\t<select title=\"Contacts\" name=\"cardAlign\" condition=\"(blockType==1 || blockType==3 || blockType==7) &amp;&amp; (showCardTitle || showCardText)\">\n        <option value=\"1\">Left</option>\n        <option value=