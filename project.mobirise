        {
          "_styles": {
            "padding-top": "(@paddingTop * 15px)",
            "padding-bottom": "(@paddingBottom * 15px)",
            "& when (@bg-type = 'color')": {
              "background-color": "@bg-value"
            },
            "& when (@bg-type = 'image')": {
              "background-image": "url(@bg-value)",
              "& when (@overlay)": {
                ".mbr-overlay": {
                  "background": "@overlayColor",
                  "opacity": "@overlayOpacity"
                }
              }
            },
            ".mbr-row > *": {
              "@media (max-width: 992px)": {
                "padding": "0"
              }
            },
            ".title": {
              "width": "100%"
            },
            "& when (@blockType = '2'), (@blockType = '3')": {
              ".container, .container-fluid": {
                "@media (max-width: 768px)": {
                  "flex-direction": "column"
                }
              }
            },
            ".first-element": {
              "@media (max-width: 992px)": {
                "padding": "0",
                "margin": "0"
              }
            },
            "& when (@blockType = '1'), (@blockType = '2'), (@blockType = '5')": {
              ".map-block": {
                "& when (@blockType = '2'), (@blockType = '5')": {
                  "width": "100%"
                },
                "@media (max-width: 992px)": {
                  "width": "100%",
                  "margin-bottom": "2rem"
                }
              },
              "iframe, amp-iframe": {
                "min-height": "(@mapHeight * 1px)",
                "width": "100%"
              }
            },
            "& when (@blockType = '1'), (@blockType = '3'), (@blockType = '7')": {
              ".text-block": {
                "@media (max-width: 992px)": {
                  "text-align": "center"
                },
                ".text-item": {
                  "width": "100%"
                }
              },
              ".icons-list": {
                ".amp-iconfont": {
                  "color": "@primaryColor",
                  "transition": "color .2s",
                  "font-size": "(@iconsSize * 1rem)",
                  "&:hover": {
                    "color": "@secondaryColor"
                  }
                }
              }
            },
            "& when (@blockType = '2'), (@blockType = '3'), (@blockType = '6')": {
              ".form-block": {
                "@media (max-width: 992px)": {
                  "padding": "0",
                  "margin": "0"
                }
              },
              ".mbr-form": {
                "position": "relative",
                "padding": "3rem",
                ".mbr-overlay": {
                  "opacity": "@formBgOpacity",
                  "background": "@formBgColor"
                },
                "@media (max-width: 992px)": {
                  "width": "65%"
                },
                "@media (max-width: 768px)": {
                  "width": "100%"
                },
                ".fieldset": {
                  "input, textarea": {
                    "color": "@labelsColor !important",
                    "border-bottom": "1px solid @labelsColor"
                  },
                  "input::-webkit-input-placeholder": {
                    "color": "fade(@labelsColor, 50%)"
                  },
                  "input::-moz-placeholder": {
                    "color": "fade(@labelsColor, 50%)"
                  },
                  "textarea::-webkit-input-placeholder": {
                    "color": "fade(@labelsColor, 50%)"
                  },
                  "textarea::-moz-placeholder": {
                    "color": "fade(@labelsColor, 50%)"
                  },
                  "textarea": {
                    "min-height": "200px"
                  }
                }
              }
            }
          },
